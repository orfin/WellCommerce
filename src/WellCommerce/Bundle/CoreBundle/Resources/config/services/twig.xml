<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="twig.extension.string_loader" class="Twig_Extension_StringLoader">
            <tag name="twig.extension"/>
        </service>

        <service id="twig.extension.address_formatter" class="WellCommerce\Bundle\CoreBundle\Twig\AddressFormatterExtension">
            <tag name="twig.extension"/>
        </service>
    </services>
</container>


